{
  "name": "obsidian-mcp",
  "version": "1.1.0",
  "description": "MCP server for AI assistants to interact with Obsidian vaults",
  "type": "module",
  "main": "build/main.js",
  "bin": {
    "obsidian-mcp": "./build/main.js"
  },
  "files": [
    "build",
    "README.md",
    "LICENSE"
  ],
  "exports": {
    ".": "./build/main.js",
    "./utils/*": "./build/utils/*.js",
    "./resources/*": "./build/resources/*.js"
  },
  "peerDependencies": {
    "@modelcontextprotocol/sdk": "^1.0.4"
  },
  "dependencies": {
    "@types/better-sqlite3": "^7.6.13",
    "@types/chokidar": "^1.7.5",
    "better-sqlite3": "^12.0.0",
    "chalk": "^5.4.1",
    "chokidar": "^4.0.3",
    "commander": "^14.0.0",
    "yaml": "^2.6.1",
    "zod": "^3.22.4",
    "zod-to-json-schema": "^3.24.1"
  },
  "devDependencies": {
    "@modelcontextprotocol/sdk": "^1.0.4",
    "@types/bun": "latest",
    "@types/jest": "^30.0.0",
    "@types/node": "^20.19.1",
    "jest": "^30.0.2",
    "tsx": "^4.20.3",
    "typescript": "^5.8.3"
  },
  "scripts": {
    "build": "tsc && node -e \"require('fs').chmodSync('build/main.js', '755')\"",
    "start": "node build/main.js",
    "dev": "tsx src/main.ts",
    "prepublishOnly": "npm run build",
    "inspect": "npx @modelcontextprotocol/inspector node ./build/main.js"
  },
  "keywords": [
    "obsidian",
    "mcp",
    "ai",
    "notes",
    "knowledge-management"
  ],
  "author": "Steven Stavrakis",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/StevenStavrakis/obsidian-mcp"
  },
  "engines": {
    "node": ">=16"
  }
}
